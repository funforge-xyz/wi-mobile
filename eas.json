{
  "cli": {
    "version": ">= 16.8.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENVIRONMENT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "credentialsSource": "local"
      },
      "env": {
        "ENVIRONMENT": "development"
      }
    },
    "production": {
     "env": {
        "ENVIRONMENT": "production"
      },
      "ios": {
        "credentialsSource": "local"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "local"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
